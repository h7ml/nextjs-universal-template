{
  "name": "nextjs-universal-template",
  "version": "1.0.0",
  "description": "Universal Next.js template for Vercel, Deno Deploy, and Cloudflare Pages",
  "private": true,
  "author": {
    "name": "h7ml",
    "url": "https://github.com/h7ml"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:h7ml/nextjs-universal-template.git"
  },
  "homepage": "https://github.com/h7ml/nextjs-universal-template#readme",
  "bugs": {
    "url": "https://github.com/h7ml/nextjs-universal-template/issues"
  },
  "keywords": [
    "nextjs",
    "next.js",
    "react",
    "typescript",
    "trpc",
    "drizzle",
    "postgresql",
    "vercel",
    "deno",
    "cloudflare",
    "full-stack",
    "template",
    "boilerplate",
    "authentication",
    "jwt",
    "dashboard",
    "charts",
    "tailwindcss",
    "radix-ui"
  ],
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "pnpm lint && pnpm type-check && pnpm format:check",
    "clean": "rm -rf .next .turbo node_modules",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/db/seed.ts",
    "db:fix": "tsx scripts/fix-db.ts",
    "deploy:vercel": "./scripts/deploy.sh vercel",
    "deploy:cloudflare": "./scripts/deploy.sh cloudflare",
    "deploy:deno": "./scripts/deploy.sh deno",
    "build:cf": "pnpm build && rm -rf .next/cache",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:dev": "npx wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat",
    "pages:deploy": "pnpm pages:build && wrangler pages deploy .vercel/output/static --project-name=nextjs-universal-template",
    "cf:build": "pnpm build:cf",
    "cf:deploy": "wrangler pages deploy .next --project-name=nextjs-universal-template",
    "cf:preview": "wrangler pages dev .next"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/typography": "^0.5.15",
    "@tanstack/react-query": "^5.90.2",
    "@trpc/client": "^11.6.0",
    "@trpc/react-query": "^11.6.0",
    "@trpc/server": "^11.6.0",
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.5",
    "echarts": "^5.5.0",
    "echarts-for-react": "^3.0.2",
    "framer-motion": "^12.23.22",
    "jose": "^6.1.0",
    "lucide-react": "^0.453.0",
    "nanoid": "^5.1.5",
    "next": "^14.2.0",
    "next-themes": "^0.4.6",
    "postcss": "^8.4.47",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.15.2",
    "superjson": "^1.13.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.4",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "prettier": "^3.3.3",
    "tsx": "^4.20.6",
    "wrangler": "^4.45.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "volta": {
    "node": "20.11.1"
  },
  "files": [
    "src",
    "public",
    "deno_server.ts",
    "deno.json",
    "drizzle.config.ts",
    "next.config.js",
    "tailwind.config.ts",
    "postcss.config.js",
    "tsconfig.json",
    "README.md",
    "LICENSE"
  ]
}
